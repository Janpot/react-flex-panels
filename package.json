{
  "name": "react-flex-panels-workspace",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "build": "pnpm -r build",
    "start": "pnpm -F docs start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typescript": "pnpm -r typescript",
    "prettier": "prettier --write \"**/*.{js,ts,jsx,tsx,json,md,mdx,css}\"",
    "release": "pnpm -F \"./packages/**\" build && pnpm -r publish"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.31.0",
    "eslint-config-next": "^15.4.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "pkg-pr-new": "^0.0.54",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.37.0"
  },
  "packageManager": "pnpm@10.13.1"
}
