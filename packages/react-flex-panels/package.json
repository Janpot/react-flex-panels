{
  "name": "react-flex-panels",
  "version": "1.0.3",
  "description": "A React component library for flexible panel layouts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typescript": "tsc --noEmit"
  },
  "keywords": [
    "react",
    "flex",
    "panels",
    "layout",
    "components"
  ],
  "author": "Jan Potoms",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "github:Janpot/react-flex-panels"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
}
